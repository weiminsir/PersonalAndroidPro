<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weimin.android" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PictureCopy"
            android:label="@string/title_activity_picture_copy" />
        <activity
            android:name=".StartMusic"
            android:label="@string/title_activity_start_music" />

        <service android:name=".MusicService" />

        <activity
            android:name=".VideoStart"
            android:label="@string/title_activity_video_start" />
        <activity
            android:name=".CameraTake"
            android:label="@string/title_activity_camera_take" />

        <provider
            android:name=".MyProvider"
            android:authorities="com.android.myContentProvider" />

        <activity
            android:name=".ProviderManager"
            android:label="@string/title_activity_provider_manager" >
        </activity>
        <activity
            android:name=".AcauireMessage"
            android:label="@string/title_activity_acauire_message" >
        </activity>
        <activity
            android:name=".CustomObserver"
            android:label="@string/title_activity_custom_observer" >
        </activity>
        <activity
            android:name=".FragementActivity"
            android:label="@string/title_activity_fragement" >
        </activity>
        <activity
            android:name=".Animation"
            android:label="@string/title_activity_animation" >
        </activity>
    </application>

</manifest>
